<% provide(:title, 'Enter Team') %>
<h1>Enter Team Name for Preassigned Project</h1>

<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <%= form_for(:teamname, url: preassign_path) do |f| %>
     
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
  
      <div class="col-md-6 col-md-offset-3">
        <%= f.submit "Submit", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>